{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ES2015",
    "moduleResolution": "Bundler",
    "outDir": ".dist",
    "jsx": "react",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    }
  }
}
